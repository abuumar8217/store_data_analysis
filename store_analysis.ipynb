        "df['IncomeBin'] = pd.cut(df['Income'], bins=bins, labels=labels, right=False)\n",
        "\n",
        "# ave spending per bin\n",
        "avg_spending_income_bin = df.groupby('IncomeBin')['TotalSpent'].mean().reset_index()\n",
        "avg_spending_income_bin"
