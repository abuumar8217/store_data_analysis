        "purchase_df['Percentage'] = (purchase_df['Total Purchases'] / purchase_df['Total Purchases'].sum()) * 100\n",
        "\n",
        "purchase_df['Label'] = purchase_df.apply(lambda row: f\"({row['Percentage']:.2f}%)\", axis=1)\n",
        "\n",
        "purchase_df"
